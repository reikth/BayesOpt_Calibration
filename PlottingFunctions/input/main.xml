			<option name="simulatedEIR"/>   <!-- id 36 -->
			<option name="expectedDirectDeaths"/>   <!-- id 74 -->
			<option name="expectedIndirectDeaths"/> <!-- id 76 -->
		</SurveyOptions>
		<surveys detectionLimit="40">
			<surveyTime repeatStep="1y" repeatEnd="@end_year@-01-01">
